.shoppingCar
  position :fixed
  left :0
  bottom :0
  width :100%
  height :1.08rem
  background-color :#141d27
  display :flex
  align-items :center
  .total
    width :100%
    display :flex
    align-items :center
    .car-box
      position :relative
      .bg-car
        width :1rem
        height :1rem
        background-color :#141d27
        margin :-.48rem .36rem 0 .36rem
        border-radius :50%
      .car
        position:absolute
        left:.45rem
        top:-.24rem
        width :.8rem
        height :.8rem
        text-align :center
        line-height :.8rem
        font-size:.48rem
        background-color :rgba(0,0,0,.2)
        color:rgba(255,255,255,.4)
        border-radius :50%
    .totalMoney
      font-size:.32rem
      font-weight:700
      color:rgba(255,255,255,.4)
    .line
      width :1px
      height :.6rem
      background-color :rgba(255,255,255,.1)
      margin :0 .24rem
    .distribution
      font-size:.24rem
      font-weight :200
      color:rgba(255,255,255,.4)
  .startSend
    width :2.1rem
    background-color :rgb(43,51,59)
    p
      font-size:.28rem
      color:rgba(255,255,255,.4)
      width :100%
      line-height :1.08rem
      text-align :center
